{
    "rewrites": [
        {
            "source": "/api/notamdata/(.*)",
            "destination": "https://www.notammap.org/notamdata/$1"
        },
        {
            "source": "/api/weather",
            "destination": "https://ims.gov.il/he/aviation_data"
        },
        {
            "source": "/(calculator|navigation|config|checklist)(/.*)?",
            "destination": "/index.html"
        }
    ]
}